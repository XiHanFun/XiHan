<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net7.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<RepositoryUrl>https://github.com/XiHanFun/XiHan.Framework</RepositoryUrl>
		<AssemblyVersion>0.2.5</AssemblyVersion>
		<FileVersion>0.2.5</FileVersion>
		<Title>XiHan.Subscriptions</Title>
		<Version>0.2.5</Version>
		<Authors>ZhaiFanhua</Authors>
		<Copyright>Copyright Â© ZhaiFanhua All Rights Reserved.</Copyright>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<Description>XiHan.Subscriptions</Description>
		<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
		<PackageIcon>readme.png</PackageIcon>
		<PackageReadmeFile>readme.md</PackageReadmeFile>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
	</PropertyGroup>

	<ItemGroup>
	  <ProjectReference Include="..\XiHan.Commons\XiHan.Commons.csproj" />
	</ItemGroup>

  <ItemGroup>
    <None Update="LICENSE">
      <PackagePath>\</PackagePath>
      <Pack>True</Pack>
    </None>
    <None Update="readme.md">
      <PackagePath>\</PackagePath>
      <Pack>True</Pack>
    </None>
    <None Update="readme.png">
      <PackagePath>\</PackagePath>
      <Pack>True</Pack>
    </None>
  </ItemGroup>

</Project>
