<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	<DebugType>embedded</DebugType>
	<GenerateDocumentationFile>True</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Cache\**" />
    <EmbeddedResource Remove="Cache\**" />
    <None Remove="Cache\**" />
  </ItemGroup>

  <ItemGroup>
    <Content Remove="C:\Users\zhaifanhua\.nuget\packages\ip2region.net\1.0.10\contentFiles\any\net6.0\Data\ip2region.xdb" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="ConfigData\Log4Net.config" />
    <None Remove="ConfigData\NLog.config" />
    <None Remove="ConfigData\Serilog.json" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="ConfigData\Log4Net.config">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
