	</ItemGroup>
	<ItemGroup>
	  <None Update="ConfigData\ip2region.xdb">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </None>
	</ItemGroup>
</Project>
